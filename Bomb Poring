//================
// Prize
//================

Prize:
	set @prize2,rand (1,3);
	if (@prize2 == 1) goto P12;
	if (@prize2 == 2) goto P22;
	if (@prize2 == 3) goto P23;

P12:
	mes "You got 50 Poring Coin";
	getitem 7227,50;
	close;
P22:
	mes "You got 100 Cash Point"; 
	atcommand "!cash 100";
	close;
P23:
	mes "You got 30 TCG Card"; 
	getitem 7227,30;
	close;

}
